import Vue from "vue/dist/vue.esm";
import App from "./App";

Vue.config.debug = true;

document.addEventListener("DOMContentLoaded", () => {
  const articleId = document.getElementById("article").dataset.articleId;
  const app = new Vue({
    render: h => h(App, {
      props: {
        articleId: articleId
      }
    })
  }).$mount("article-comments");
});
